    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "name": "Debug via example file (ts-node)",
            "request": "launch",
            "command": "ts-node -- ${workspaceFolder}/example/index.ts",
            "cwd": "${fileDirname}",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "<node_modules>/**",
                "${workspaceFolder}/node_modules/**/*.js",
    "${workspaceFolder}/example/node_modules/**/*.js"
            ],
          }
    ]
}